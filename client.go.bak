package daobackup

import "os"

type BackupClient struct {
}

func (b BackupClient) BackupFile(f os.File) (err error) {
	stat, err := f.Stat()
	if err != nil {
		return err
	}
	filemeta := FileMeta{
		Type: Normal, // TODO: don't assume non-normal filatypes
		Name: stat.Name(),
		Size: stat.Size(),
		Mode: stat.Mode().String(),
	}
}
